#3D Graph
[Comment=****** 3D Graph Initialization ******]
[Tab=3dgraph]
a=1
b=2
c=3
highres=50
lowres=20
[FullWindow=on]
[3DEqAllEmpty]
[3DGraphMode=euclidean]
[RunScript="3dgraph_defaults.gcs"]
[Comment=****** 3D Graph Euclidean Testing ******]
[3DEq=a:"-2+((cos(sqrt(((x+0)^2)+((y+0)^2))) + cos(sqrt(((x+.913*2pi)^2)+((y+0)^2))) + cos(sqrt(((x-.913*2pi)^2)+((y+0)^2)))))"]
[3DEq=b:"4*cos (3*sqrt(x^2 + y^2))*e^(-0.3*(sqrt(x^2 + y^2)))"]
[3DEq=c:"2+(sin (x*y/4)+frand(0:1))"]
[3DEqOn=a:on]
[3DEqOn=b:on]
[3DEqOn=c:on]
[3DRecomputeAll]
[3DRotateXIncrease=30]
[3DRotateYIncrease=30]
[3DRotateZIncrease=30]
[3DZoomIn]
[3DZoomIn]
[3DZoomIn]
[3DZoomIn]
[3DRotateXDecrease=180]
[3DRotateYDecrease=180]
[3DRotateZDecrease=180]
[3DZoomOut]
[3DZoomOut]
[3DZoomOut]
[3DZoomOut]
[3DXmin=-10]
[3DXmax=10]
[3DYmin=-10]
[3DYmax=10]
[3DEqWireMode=b:W]
[3DEqWireMode=a:C]
[3DEqShadeMode=a:elevation]
[3DEqWireMode=c:CW]
[3DEqShadeMode=c:elevation]
[3DRecomputeAll]
[3DZoomIn]
[3DZoomIn]
[3DZoomIn]
[3DRotateXIncrease=40]
[3DZoomIn]
[3DRotateXIncrease=30]
[3DRotateYIncrease=30]
[3DRotateZIncrease=30]
[3DZoomOut]
[3DZoomOut]
[3DZoomOut]
[3DZoomOut]
[3DCopyGraph]
[3DReCenterView]
[3DZoomOut]
[3DZoomOut]
[3DSaveBitmap="output\3dgraph_1.bmp"]
[3DExportDXF="output\3dgraph_1.dxf"]
[3DXintervals=highres]
[3DYintervals=highres]
[3DWireResolution=10]
[3DRecomputeAll]
[3DSaveBitmap="output\3dgraph_2.bmp"]
[3DExportDXF="output\3dgraph_2.dxf"]
[Comment=****** 3D Graph Polar Testing ******]
[RunScript="3dgraph_defaults.gcs"]
[3DGraphMode=polar]
[3DEqAllEmpty]
a=4
b=5
c=6
[3DEq=a:"-2+(cos(3*r)*cos(5*t)*e^(-.3r)*3)"]
[3DEq=b:"(r+t)*.5"]
[3DEq=c:"2+(sin(r*t))"]
[3DEqOn=a:on]
[3DEqOn=b:on]
[3DEqOn=c:on]

[3DXmin=0]
[3DXmax=2]
[3DYmin=0]
[3DYmax=pi]
[3DRecomputeAll]
[3DRotateXIncrease=30]
[3DRotateYIncrease=30]
[3DRotateZIncrease=30]
[3DZoomIn]
[3DZoomIn]
[3DZoomIn]
[3DZoomIn]
[3DRotateXDecrease=180]
[3DRotateYDecrease=180]
[3DRotateZDecrease=180]
[3DZoomOut]
[3DZoomOut]
[3DZoomOut]
[3DZoomOut]
[3DXmax=5]
[3DYmax=pi*2]
[3DEqWireMode=b:W]
[3DEqWireMode=a:C]
[3DEqShadeMode=a:elevation]
[3DEqWireMode=c:CW]
[3DEqShadeMode=c:elevation]
[3DRecomputeAll]
[3DZoomIn]
[3DZoomIn]
[3DZoomIn]
[3DZoomIn]
[3DRotateXIncrease=30]
[3DRotateYIncrease=30]
[3DRotateZIncrease=30]
[3DZoomOut]
[3DZoomOut]
[3DZoomOut]
[3DZoomOut]
[3DCopyGraph]
[3DReCenterView]

[3DSaveBitmap="output\3dgraph_3.bmp"]
[3DExportDXF="output\3dgraph_3.dxf"]
[3DXintervals=highres]
[3DYintervals=highres]
[3DWireResolution=10]
[3DRecomputeAll]
[3DSaveBitmap="output\3dgraph_4.bmp"]
[3DExportDXF="output\3dgraph_4.dxf"]
